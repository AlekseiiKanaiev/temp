{
    "product": "bitbucket",
    "development": {
        "clientId": "$CLIENTID",
        "clientSecret": "$CLIENTSECRET",
        "port": 3000,
        "errorTemplate": true,
        "store": {
            "type": "postgres",
            "url": "$DATABASE_URL"
        },
        "snyk": {
            "baseUrl": "https://api.snyk.io/v1/",
            "snykOauthUrl": "https://id.snyk.io/oauth/token",
            "snykAnalyticsKey" : "$ANALYTICS"
        },
        "localBaseUrl": "$APP_URL"
    },
    "stage": {
        "port": "$PORT",
        "errorTemplate": true,
        "localBaseUrl": "$APP_URL",
        "clientId": "$CLIENTID",
        "clientSecret": "$CLIENTSECRET",
        "store": {
            "type": "postgres",
            "url": "$DATABASE_URL"
        },
        "whitelist": [
            "bitbucket.org",
            "api.bitbucket.org"
        ],
        "snyk": {
            "baseUrl": "https://api.snyk.io/v1/",
            "snykOauthUrl": "https://id.snyk.io/oauth/token",
            "snykAnalyticsKey" : "$ANALYTICS"
        }
    },
    "production": {
        "port": "$PORT",
        "errorTemplate": true,
        "clientId": "$CLIENTID",
        "clientSecret": "$CLIENTSECRET",
        "localBaseUrl": "$APP_URL",
        "store": {
            "type": "postgres",
            "url": "$DATABASE_URL"
        },
        "whitelist": [
            "bitbucket.org",
            "api.bitbucket.org"
        ],
        "snyk": {
            "baseUrl": "https://api.snyk.io/v1/",
            "snykOauthUrl": "https://id.snyk.io/oauth/token",
            "snykAnalyticsKey" : "$ANALYTICS"
        }
    }
}